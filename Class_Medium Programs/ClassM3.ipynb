        "class Vector:\n",
        "  def __init__(self,x,y):\n",
        "    self.x=x\n",
        "    self.y=y\n",
        "  def magnitude(self):\n",
        "    magnitude=math.sqrt(self**x +self**y)\n",
        "  def dot_product(self,other):\n",
        "    dot_product=self.x * other.x + self.y * other.y"
