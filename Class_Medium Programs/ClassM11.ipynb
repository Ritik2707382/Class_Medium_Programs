        "import math\n",
        "class Points:\n",
        "  def __init__(self,x,y):\n",
        "    self.x=x\n",
        "    self.y=y\n",
        "  def calculate(self,other):\n",
        "    distance=math.sqrt((self.x-other.x)**2 + (self.y-other.y)**2)\n",
        "    print(\"Distance between two Points:\",distance)"
